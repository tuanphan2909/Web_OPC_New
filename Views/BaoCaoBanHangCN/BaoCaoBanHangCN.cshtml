    <script>
        $(document).ready(function () {
            // Gọi hàm tính tổng cho trang hiện tại khi trang được nạp hoặc vẽ lại
            calculateTotal();

            function calculateTotal() {
                var totalColumns = new Array(47).fill(0); // Khởi tạo mảng chứa tổng của 39 cột

                $('#example tbody tr:not(#total-row)').each(function () {
                    var cells = $(this).find('td');

                    cells.each(function (index) {
                        if (index > 0) { // Bỏ qua cột đầu tiên (Tên Đơn Vị)
                            var columnValue = parseFloat($(this).text().replace(/,/g, ''));

                            if (!isNaN(columnValue)) {
                                totalColumns[index] += columnValue;
                            }
                        }
                    });
                });

                // Hiển thị giá trị tổng từng cột lên view
                for (var i = 1; i < totalColumns.length; i++) {
                    if (![3, 6, 9, 12, 15, 20, 23, 26, 29, 32, 35, 40, 46].includes(i)) { // Kiểm tra xem cột có cần tính tổng không
                        $('#total-row td:eq(' + (i) + ')').text(formatNumber(totalColumns[i]));
                    }
                }




                // Gọi hàm calculateRatio cho các cặp doanh thu và khoản khác tương tự
            }

            function formatNumber(number) {
                return number.toLocaleString('en-US');
            }




            // Khởi tạo DataTables
            var table = $('#example').DataTable();

            // Gọi hàm tính tổng cho trang hiện tại khi trang được vẽ lại
            table.on('draw.dt', function () {
                calculateTotal();
            });
        });

