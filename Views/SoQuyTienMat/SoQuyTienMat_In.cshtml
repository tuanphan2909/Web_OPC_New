                                                    <td data-t="n" style="text-align:right;">@String.Format("{0:n0}", Convert.ToDecimal(row2["Du_Cuoi"])).Replace(",", ".")</td>
                                                </tr>
                                                decimal Thu = Convert.ToDecimal(row2["PS_No"]);
