                                                                <td style="text-align:left;color:black">KHÁC(DV + NVL + TM KHÁC)</td>
                                                            }else{
                                                                <td></td>
                                                            }

                                                            <td style="text-align:right;color:black">@FormatValue(row3["CTY"])</td>
                                                            <td style="text-align:right;color:black">@FormatValue(row3["PKD"])</td>
                                                            <td style="text-align:right;color:black">@FormatValue(row3["CNHCM"])</td>
                                                            <td style="text-align:right;color:black">@FormatValue(row3["CNCT"])</td>
                                                            <td style="text-align:right;color:black">@FormatValue(row3["CNTG"])</td>
                                                            <td style="text-align:right;color:black">@FormatValue(row3["CNMD"])</td>
                                                            <td style="text-align:right;color:black">@FormatValue(row3["CNVT"])</td>
                                                            <td style="text-align:right;color:black">@FormatValue(row3["CNNT"])</td>
                                                            <td style="text-align:right;color:black">@FormatValue(row3["CNDN"])</td>
