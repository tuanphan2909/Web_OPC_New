                        <tbody>
                            @functions {
                                private string FormatValue(object value)
                                {
                                    if (Convert.IsDBNull(value))
                                    {
                                        return string.Empty;
                                    }
                                    else
                                    {
                                        decimal decimalValue = Convert.ToDecimal(value);
                                        return String.Format("{0:n0}", decimalValue).Replace(",", ".");
                                    }
                                }

                                private decimal GetDecimalValue(object value)
                                {
                                    if (Convert.IsDBNull(value))
                                    {
                                        return 0;
                                    }
                                    else
                                    {
                                        return Convert.ToDecimal(value);
                                    }
                                }
                            }


                            @{
                                // Biến để lưu tổng của cột TỔNG
                                decimal grandTotalRow = 0;


                                foreach (DataRow row3 in Model.Tables[0].Rows)
                                {
                                    // Tính tổng cho mỗi hàng
                                    var totalRow = GetDecimalValue(row3["CTY"]) + GetDecimalValue(row3["PKD"]) + GetDecimalValue(row3["CNHCM"]) + GetDecimalValue(row3["CNCT"]) +
                                                   GetDecimalValue(row3["CNTG"]) + GetDecimalValue(row3["CNMD"]) + GetDecimalValue(row3["CNVT"]) + GetDecimalValue(row3["CNNT"]) +
                                                   GetDecimalValue(row3["CNDN"]) + GetDecimalValue(row3["CNNA"]) + GetDecimalValue(row3["CNHN"]);

                                    // Cộng dồn vào tổng của cột TỔNG
                                    grandTotalRow += totalRow;

                                                        <tr>
                                                            <td style="text-align:center;color:black">@row3["STT"]</td>
                                                            @if (row3["Loai_TP"].ToString() == "OPC")
                                                            {
                                                                <td style="text-align:left;color:black">OPC</td>
                                                            }
                                                            else if (row3["Loai_TP"].ToString() == "TP07")
                                                            {
                                                                <td style="text-align:left;color:black">SỦI</td>
                                                            }
                                                            else if (row3["Loai_TP"].ToString() == "HH04")
                                                            {
                                                                <td style="text-align:left;color:black">OPC BD</td>
                                                            }
                                                            else if (row3["Loai_TP"].ToString() == "HH25")
                                                            {
                                                                <td style="text-align:left;color:black">TW25</td>
                                                            }
                                                            else if (row3["Loai_TP"].ToString() == "TP08")
                                                            {
                                                                <td style="text-align:left;color:black">PHIẾN</td>
                                                            }
                                                            else if (row3["Loai_TP"].ToString() == "GC")
                                                            {
                                                                <td style="text-align:left;color:black">GIA CÔNG</td>
                                                            }
                                                            else if (row3["Loai_TP"].ToString() == "CD")
                                                            {
                                                                <td style="text-align:left;color:black">TPGC OPC BD</td>
                                                            }
                                                            else if (row3["Loai_TP"].ToString() == "KH")
